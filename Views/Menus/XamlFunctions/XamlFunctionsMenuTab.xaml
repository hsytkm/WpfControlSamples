<TabControl x:Class="WpfControlSamples.Views.Menus.XamlFunctionsMenuTab"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfControlSamples.Views.Menus"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Style="{StaticResource MyTabControlStyle2}" >

    <TabItem Header="Markup" >
        <local:MarkupPage />
    </TabItem>

    <TabItem Header="BindingSource" >
        <local:BindingSourcePage />
    </TabItem>

    <TabItem Header="ValueConv" >
        <local:ValueConverterPage />
    </TabItem>

    <TabItem Header="DependencyProp" >
        <local:DependencyPropertyPage />
    </TabItem>

    <TabItem Header="AttachedProp" >
        <local:AttachedPropertyPage />
    </TabItem>

    <TabItem Header="MarkupExt" >
        <local:MarkupExtensionPage />
    </TabItem>

    <TabItem Header="EventToCommand" >
        <local:BlankPage />
    </TabItem>

    <TabItem Header="MultiValueConv" >
        <local:MultiValueConverterPage />
    </TabItem>

</TabControl>
