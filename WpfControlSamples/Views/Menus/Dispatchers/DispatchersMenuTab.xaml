<TabControl x:Class="WpfControlSamples.Views.Menus.DispatchersMenuTab"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfControlSamples.Views.Menus"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Style="{StaticResource MyTabControlStyle2}" >

    <TabItem Header="DispatcherObject" >
        <local:DispatcherObjectPage />
    </TabItem>

    <TabItem Header="DispatcherTimer" >
        <local:DispatcherTimerPage />
    </TabItem>

    <TabItem Header="UnhandledException" >
        <local:DispatcherUnhandledExceptionPage />
    </TabItem>

    <TabItem Header="DispatcherCollection" >
        <local:DispatcherCollectionPage />
    </TabItem>

</TabControl>
