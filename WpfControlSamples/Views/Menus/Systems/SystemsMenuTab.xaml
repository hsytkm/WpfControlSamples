<TabControl x:Class="WpfControlSamples.Views.Menus.SystemsMenuTab"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfControlSamples.Views.Menus"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Style="{StaticResource MyTabControlStyle2}" >

    <TabItem Header="Top" >
        <local:SystemTopPage />
    </TabItem>

    <TabItem Header="Environment.Propetries" >
        <local:EnvironmentPage />
    </TabItem>

    <TabItem Header="Environment.Directries" >
        <local:EnvDirectoryPage />
    </TabItem>

    <TabItem Header="Assembly" >
        <local:AssemblyPage />
    </TabItem>

    <TabItem Header="FileVersionInfo" >
        <local:FileVerInfoPage />
    </TabItem>

    <TabItem Header="PropetriesResources" >
        <local:PropetriesResourcesPage />
    </TabItem>

    <TabItem Header="PropetriesSettings" >
        <local:PropetriesSettingsPage />
    </TabItem>
    
</TabControl>
