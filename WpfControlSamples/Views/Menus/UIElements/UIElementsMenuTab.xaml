<TabControl x:Class="WpfControlSamples.Views.Menus.UIElementsMenuTab"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfControlSamples.Views.Menus"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Style="{StaticResource MyTabControlStyle2}" >

    <TabItem Header="Transforms" >
        <local:TransformsPage />
    </TabItem>

    <TabItem Header="Effects" >
        <local:EffectsPage />
    </TabItem>

    <TabItem Header="Clip" >
        <local:ClipPage />
    </TabItem>

    <TabItem Header="Opacity" >
        <local:OpacityPage />
    </TabItem>

    <TabItem Header="VisualBrush" >
        <local:VisualBrushPage />
    </TabItem>

    <TabItem Header="SaveToImage" >
        <local:SaveToImagePage />
    </TabItem>

</TabControl>
